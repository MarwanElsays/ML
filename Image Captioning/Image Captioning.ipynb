   ],
   "source": [
    "processed_captions = captions['caption'].apply(lambda x: x.lower())                    # put into lower case\n",
    "processed_captions = processed_captions.apply(lambda x: re.sub(r\"[^A-Za-z ]\", \"\", x))  # remove non-alphabetic and non-space characters\n",
    "processed_captions = processed_captions.apply(lambda x: re.sub(r\" +\", \" \", x))         # remove multiple spaces\n",
    "processed_captions = processed_captions.apply(lambda x: x.strip())                     # remove leading and trailing spaces\n",
    "\n",
    "# remove single character words except for 'a' in the middle or start of sentence\n",
    "processed_captions = processed_captions.apply(lambda x: re.sub(r\" [^a] \", \" \", x))\n",
    "processed_captions = processed_captions.apply(lambda x: re.sub(r\"(^[^a] | .$)\", \"\", x))\n",
    "\n",
    "processed_captions = \"begintag \" + processed_captions + \" endtag\"\n",
    "\n",
    "processed_captions"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Tokenize Captions, Build Vocabulary & Prepare Output Labels"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "False"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "tokenizer = tf.keras.preprocessing.text.Tokenizer()\n",
    "tokenizer.fit_on_texts(processed_captions)\n",
    "vocab_size = len(tokenizer.word_index) + 1\n",
    "\n",
    "sequences = tokenizer.texts_to_sequences(processed_captions)\n",
    "\n",
    "max_length = max(map(len, sequences))\n",
    "\n",
    "# take only one caption for every image\n",
    "sequences = [sequences[i] for i in range(len(sequences)) if i % 5 == 0]\n",
    "\n",
    "caption_lengths = [len(sequence) for sequence in sequences]\n",
    "\n",
    "next_word = np.array([sequences[i][j] for i in range(len(sequences)) for j in range(1, len(sequences[i]))])\n",
    "\n",
    "sequences = [sequences[i][:j] for i in range(len(sequences)) for j in range(1, len(sequences[i]) + 1)]\n",
    "sequences = [sequence for sequence in sequences if sequence[len(sequence) - 1] != 3]\n",
    "sequences = pad_sequences(sequences, max_length, padding='post')\n",
    "\n",
    "next_word.shape[0] == sequences.shape[0]"
   ]
  },
